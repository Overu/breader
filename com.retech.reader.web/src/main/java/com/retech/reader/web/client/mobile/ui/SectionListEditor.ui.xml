<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui"
  xmlns:c='urn:import:com.google.gwt.user.cellview.client' xmlns:s="urn:import:org.cloudlet.web.mvp.shared.rpc">

  <ui:style>
    .scrollable {
      height: 300px;
      border: 1px solid #ccc;
      text-align: left;
    }
  </ui:style>

  <g:HTMLPanel>
    <div>
      <s:ShowMorePagerPanel addStyleNames='{style.scrollable}' ui:field='pagerPanel' />
    </div>
    <div>
      <s:RangeLabelPager ui:field='rangeLabelPager' />
    </div>
  </g:HTMLPanel>
</ui:UiBinder>
