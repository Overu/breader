<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui"
  xmlns:c='urn:import:com.google.gwt.user.cellview.client' xmlns:s="urn:import:org.cloudlet.web.mvp.shared.rpc">

  <ui:style>
    .vbox {
      display: -webkit-box;
      -webkit-box-orient: vertical;
      height: 100%;
    }
    
    .header {
      display: -webkit-box;
      -webkit-box-align: center;
      -webkit-box-pack: center;
      height: 22px;
      font-size: 20px;
      background: black;
      color: white;
    }
    
    .sections {
      -webkit-box-flex: 1;
      border: 1px solid #ccc;
      height: 100%;
    }
    
    .sections>div {
      height: 100%;
    }
  </ui:style>
  <g:HTMLPanel>
    <div class="{style.vbox}">
      <g:SimplePanel ui:field="sectionsPanel" addStyleNames="{style.sections}">
        <!-- <c:CellBrowser addStyleNames='{style.browser} {style.center}' defaultColumnWidth='300' ui:field='cellBrowser' 
          /> -->
      </g:SimplePanel>
    </div>
  </g:HTMLPanel>
</ui:UiBinder>
