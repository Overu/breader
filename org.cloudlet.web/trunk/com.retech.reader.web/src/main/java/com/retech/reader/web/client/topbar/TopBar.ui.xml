<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui">
  <ui:style>
    .topBar {
      display: -webkit-box;
      height: 100%;
    }
    
    .topBar>*:first-child {
      -webkit-box-flex: 1;
      padding: 3px;
    }
    
    .topBar>*:last-child {
      line-height: 33px;
      border-right: 10px solid transparent;
    }
  </ui:style>
  <ui:image field="retechcorpLogo" />
  <g:HTMLPanel addStyleNames='{style.topBar}'>
    <div>
      <g:Image resource='{retechcorpLogo}'></g:Image>
    </div>
    <div>About</div>
  </g:HTMLPanel>
</ui:UiBinder> 