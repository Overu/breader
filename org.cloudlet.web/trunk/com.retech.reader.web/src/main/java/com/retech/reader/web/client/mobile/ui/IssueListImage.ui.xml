<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui"
  xmlns:c='urn:import:com.google.gwt.user.cellview.client' xmlns:w="urn:import:com.google.gwt.widget.client">
  <ui:with field='res' type='com.retech.reader.web.client.style.ReaderResources' />
  <ui:style>
    .div_imagery {
      display: -webkit-box;
      -webkit-box-orient: horizontal;
    }
    
    .div_imagery>div {
      cursor: pointer;
      cursor: hand;
      padding: 0 10px 0 0;
    }
    
    .div_imagery>div>img {
      width: 100%;
      height: auto;
      max-height: 260px;
      max-width: 200px;
    }
    
    .div_overflow {
      overflow: auto;
    }
  </ui:style>
  <g:HTMLPanel>
    <g:HTMLPanel addStyleNames="{style.div_imagery} {style.div_overflow}" ui:field="imageDiv">
    </g:HTMLPanel>
  </g:HTMLPanel>
</ui:UiBinder>