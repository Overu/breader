<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui">

  <ui:style>
    .root {
      left: -14px;
      top: -6px;
      position: absolute;
      border: 1px solid #DDD;
      background: white;
      width: 400px;
    }
    
    }
    @sprite 
    .stacksharingarrow {
      gwt-image: "stacksharingarrow";
      position: absolute;
      bottom: -9px;
      left: 253px;
      z-index: 101;
    }
    
    .container>div {
      float: left;
      width: 100px;
      height: 80px;
    }
    
    .container>div:HOVER {
      background-color: whiteSmoke;
    }
    
    @sprite 
    .presplit1 {
      gwt-image: "presplit1";
    }
    
    @sprite 
    .presplit2 {
      gwt-image: "presplit2";
    }
    
    @sprite 
    .presplit3 {
      gwt-image: "presplit3";
    }
    
    @sprite 
    .presplit4 {
      gwt-image: "presplit4";
    }
  </ui:style>
  <ui:image field="presplit1" />
  <ui:image field="presplit2" />
  <ui:image field="presplit3" />
  <ui:image field="presplit4" />
  <ui:image field="stacksharingarrow" />

  <g:HTMLPanel addStyleNames="{style.root}">
    <div class="{style.stacksharingarrow}"></div>
    <div ui:field="container" class="{style.container}">
      <div class="{style.presplit1}"></div>
      <div class="{style.presplit2}"></div>
      <div class="{style.presplit3}"></div>
      <div class="{style.presplit4}"></div>
    </div>
  </g:HTMLPanel>
</ui:UiBinder>