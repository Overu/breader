<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui">
  <ui:style>
    @sprite .waveToolbarDivider {
      gwt-image: "waveToolbarDivider";
    }
    
    @sprite .waveToolbar {
      gwt-image: "waveToolbarEmpty";
      height: 24px;
      overflow: hidden;
      z-index: 0;
    }
  </ui:style>
  <ui:image field="waveToolbarDivider" src="waveToolbarDivider.png"></ui:image>
  <ui:image field="waveToolbarEmpty" src="waveToolbarEmpty.png" repeatStyle="Horizontal"></ui:image>
  <g:HTMLPanel addStyleNames="{style.waveToolbar}">
  </g:HTMLPanel>
</ui:UiBinder> 