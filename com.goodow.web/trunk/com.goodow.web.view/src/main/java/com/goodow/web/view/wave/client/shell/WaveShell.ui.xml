<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui"
  xmlns:shell="urn:import:com.goodow.web.view.wave.client.shell">
  <ui:style>
    .topBar {
      height: 40px;
    }
  </ui:style>
  <g:HTMLPanel>
    <g:FlowPanel addStyleNames='{style.topBar}' ui:field="topBar">
    </g:FlowPanel>
    <shell:WidgetContainer ui:field="container"></shell:WidgetContainer>
  </g:HTMLPanel>
</ui:UiBinder> 